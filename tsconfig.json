{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "@/navigator/*": ["./src/navigator/*"],
      "@/components/*": ["./src/components/*"],
      "@/assets/*": ["./src/asset/*"],
      "@/store/*": ["./src/store/*"],
      "@/screens/*": ["./src/screens/*"],
      "@/utils/*": ["./src/utils/*"],
      "@/api/*": ["./src/api/*"],
      "@/contexts/*": ["./src/contexts/*"]
    }
  },
  "exclude": ["node_modules/", "dist"]
}
